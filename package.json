{
  "name": "inhousekitchen",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/react": "^3.1.9",
    "@storybook/react-native": "^3.1.9",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-react-native-web": "^0.3.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest-expo": "23.0.0",
    "prettier": "^1.10.2",
    "react-addons-test-utils": "^15.6.2",
    "react-native-scripts": "1.8.1",
    "react-native-storybook-loader": "^1.7.0",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.2"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007 -c src/storybook",
    "build-storybook": "build-storybook",
    "prettier": "prettier --single-quote --print-width 80 --trailing-comma all --write 'src/**/*.js'",
    "lint": "eslint --fix src/**/**/*.js"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|expo|@expo|react-navigation|native-base-shoutem-theme|@shoutem/theme|@shoutem/animation|@shoutem/ui|tcomb-form-native)"
    ],
    "setupFiles": [
      "./jest/setup.js"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^6.2.2",
    "@redux-offline/redux-offline": "^2.2.1",
    "axios": "^0.17.1",
    "color": "^2.0.1",
    "core-decorators": "^0.20.0",
    "expo": "^23.0.4",
    "faker": "^4.1.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "native-base": "^2.3.6",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-23.0.0.tar.gz",
    "react-native-calendars": "^1.16.1",
    "react-native-collapsible": "^0.10.0",
    "react-native-extended-stylesheet": "^0.8.0",
    "react-native-interactable": "^0.1.7",
    "react-native-read-more-text": "^1.0.0",
    "react-native-star-rating": "^1.0.8",
    "react-native-swiper": "^1.5.13",
    "react-native-vector-icons": "^4.5.0",
    "react-native-web": "^0.3.2",
    "react-navigation": "^1.0.0-beta.27",
    "react-navigation-redux-helpers": "^1.0.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./packages"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./src/storybook/storyLoader.js"
    }
  }
}
